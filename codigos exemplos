codigo 1 >>>

inicio
    inteiro a, b
    leia a, b

    se (a maiorigual b e b diferente 0) entao
        escreva 'Condicional verdadeira'
    senao
        escreva 'Condicional falsa'
    fimse
fim


codigo 2 >>>

inicio
    inteiro a, b, soma
    escreva "Digite dois números: "
    leia a, b
    soma igual a somar b
    escreva "A soma é: ", soma
fim


codigo 3 >>>

inicio
    inteiro contador, numero, maiornumero
    maiornumero igual 0
    escreva "Digite 5 números para verificar o maior entre eles: "
    
    para contador de 1 ate 5 faca
        leia numero
        se(numero maior maiornumero)entao
            maiornumero igual numero
        fimse
    fimpara
    
    escreva "O maior número digitado foi: ", maiornumero
fim


codigo 4 >>>

inicio
    inteiro contador
    contador igual 0

    enquanto (contador menor 5) faca
        escreva "Contador: ", contador
        contador igual  contador somar 1
    fimenquanto
fim


