fim


codigo 6 >>>

inicio
    inteiro numero, soma
    soma igual 0
    escreva "Digite números positivos para somar (digite um número negativo para sair): "
    enquanto (verdadeiro) faca
        leia numero
        se (numero menor 0) entao
            saia
        fimse
        soma igual soma somar numero
    fimenquanto
    escreva "A soma dos números positivos é: ", soma
fim


codigo 7 >>>

inicio
    inteiro numero, fatorial, i
    escreva "Digite um número: "
    leia numero
    fatorial igual 1
    para i de 1 ate numero faca
        fatorial igual  fatorial multiplicar i
    fimpara
    escreva "O fatorial de ", numero, " é: ", fatorial
fim


codigo 8 >>>

inicio
    inteiro numero, i, divisor
    divisor igual  0
    escreva "Digite um número: "
    leia numero
    para i de 1 ate numero faca
        se (numero mod i igual igual 0) entao
            divisor igual divisor somar 1
        fimse
    fimpara
    se (divisor igual igual  2) entao
        escreva numero, " é primo."
    senao
        escreva numero, " não é primo."
    fimse
fim

